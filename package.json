{
  "name": "pigskinstats.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf dist-old && mv dist dist-old && vue-cli-service build && cp -r dist-old/.git dist",
    "download": "cd scripts && python3 ./download_scores.py && python3 ./generate_json.py",
    "deploy": "cd dist && git add . && git cm --amend -m 'Build app' && git push -f"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "axios": "^0.23.0",
    "bootstrap": "^5.1.3",
    "bootstrap-vue-3": "^0.0.4",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
